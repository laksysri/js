<script>

   var student=["ravi","raju","puvi","sasi"];
   
   var tag="<ul>";
   for (var i=0;i<=student.length-1;i++)
{
    tag+="<li>"+student[i]+"</li>";
}
tag+="<li>+student[i]+"</li>";
tag+="</ul>";



document.getElementById('demo').innerHTML=student;
document.getElementById('demo1').innerHTML=student[3];

